{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "interface-name": [
      false
    ],
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "arrow-parens": [
      false
    ],
    "max-line-length": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-console": false,
    "no-empty": false,
    "no-empty-interface": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-angle-bracket-type-assertion": false,
    "jsx-no-multiline-js": false,
    "jsx-alignment": false
  },
  "rulesDirectory": []
}